{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"quotemark": [true, "double", "avoid-template"],
		"semicolon": [true, "never"],
		"indent": [true, "tabs"],
		"no-var-requires": false,
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
		"no-console": [true, "assert", "clear", "count", "debug", "dir", "dirxml", "error", "group", "groupCollapsed", "groupEnd", "info", "table", "time", "timeEnd", "trace", "warn"],
		"no-any": true,
		"ordered-imports": false,
		"max-classes-per-file": [true, 2],
		"max-line-length": [false, 150],
		"object-literal-sort-keys": false,
		"no-return-await": true,
		"ignoreDefinitionFiles": false,
		"cyclomatic-complexity": [true, 7],
		"no-magic-numbers": true
	},
	"rulesDirectory": []
}
